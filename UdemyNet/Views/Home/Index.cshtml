@model List<UdemyNet.Models.Customer> 
    <table>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Age</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                   <td>@item.Name</td>
                   <td>@item.Surname</td>
                   <td>@item.Age</td>
            </tr>
        }
    </table>