@model List<UdemyNet.Models.Customer> 

<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title>Customer-List</title>
        <link href="~/style.css"rel="stylesheet"/>
    </head>
    <body>
        <table>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Age</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                   <td>@item.Name</td>
                   <td>@item.Surname</td>
                   <td>@item.Age</td>
            </tr>
        }
        </table>

    </body>
</html>
   